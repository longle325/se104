{
  "name": "se104-app",
  "version": "1.0.0",
  "description": "UIT Lost & Found Application",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && python start_servers.py",
    "frontend": "cd frontend && npm run dev",
    "backend-simple": "cd backend && python main.py",
    "install-all": "cd frontend && npm install && cd ../backend && pip install -r requirements.txt"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
